"{\"name\": \"test_in_out\", \"result\": \"hello\", \"errors\": []}"

class Main inherits IO {
    a:Int;
    
    set(v:Int):Int {
        a <- v
    };
    
    get():Int {
        a
    };
    main():String {
        {
            out_string("Write a number");
            a:Int <- in_int();
            out_string("Write a string");
            b:String <- in_string();
            out_int(a).out_string(b);
            b;
        }
    };
};
